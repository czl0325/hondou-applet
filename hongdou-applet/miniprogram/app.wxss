/**app.wxss**/
@import "iconfont.wxss";

.container {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

text {
  color: #333;
  font-size: 32rpx;
}

button {
  background-color: #FE765E;
  color: white;
  font-size: 32rpx;
  text-align: center;
  border-radius: 10rpx;
  border: none;
  line-height: 1;
  margin: 0;
}

button::after {
  border: none;
  padding: 0;
}

/* input {
  outline: none;
  list-style: none;
  font-size: 32rpx;
} */

page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.cell {
  width: 100%;
  padding: 0 20rpx;
  height: 100rpx;
  min-height: 100rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #ddd;
  background-color: white;
}
